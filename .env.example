# Stripe (start med test mode)
STRIPE_SECRET_KEY=sk_test_your_stripe_test_secret_key

# NextAuth
# Generate med: openssl rand -base64 32
NEXTAUTH_SECRET=your_nextauth_secret_minimum_32_characters
NEXTAUTH_URL=http://localhost:3000

# Google OAuth
# Få credentials fra: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Whitelist email (kun denne email kan logge ind)
ALLOWED_EMAIL=din@email.com

# Anthropic API (til AI Chat feature)
# Få API key fra: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key
